<% for(var i=0;i<employees.length;i++){%>

<tr>
    <td><%= i+1 %></td>
    <td><%= employees[i].name %></td>
    <td><%= employees[i].birth_date %></td>
    <td><%= employees[i].gender %></td>
    <td><%= employees[i].salary %></td>
    <td>
        <a href="/update_employee?id=<%= employees[i]._id %>" class="btn border-shadow update">
        <span class="text-gradient"><i class="fa-solid fa-pen-clip"></i></span></a>

        <a class="btn border-shadow delete" data-id =<%= employees[i]._id%> >
           <span class="text-gradient"><i class="fa-solid fa-trash-can"></i></span></a>
    </td>

</tr>
<%}%>